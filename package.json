{
  "name": "autodj",
  "version": "1.0.0",
  "description": "auto dj web pages",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "devserver": "live-server --browser=firefox",
    "start": "node index.js ",
    "start:dev": "nodemon index.js",
    "watch:js": "parcel watch ./public/js/index.js --out-dir ./public/js --out-file bundle.js --public-url /js",
    "build:js": "parcel build ./public/js/index.js --out-dir ./public/js --out-file bundle.js --public-url /js"
  },
  "author": "sheila honey",
  "license": "ISC",
  "dependencies": {
    "blob-stream": "^0.1.3",
    "brfs": "^2.0.2",
    "express": "^4.17.1",
    "live-server": "^1.2.1",
    "node": "^15.14.0",
    "pdfkit": "^0.11.0"
  },
  "devDependencies": {
    "parcel-bundler": "^1.12.3",
    "prettier": "^2.2.1"
  }
}
